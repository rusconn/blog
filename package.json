{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:path": "yarn path --watch",
    "dev:generate": "npm run generate",
    "dev:next": "next dev",
    "prd": "run-s prd:*",
    "prd:path": "yarn path",
    "prd:generate": "npm run generate",
    "prd:build": "next build",
    "prd:next": "next start",
    "fix": "run-s fix:*",
    "fix:path": "yarn path",
    "fix:generate": "npm run generate",
    "fix:type": "yarn typecheck",
    "fix:lint": "yarn lint --fix",
    "fix:format": "yarn format",
    "lint-staged": "lint-staged --concurrent false",
    "path": "pathpida -o ./src/libs --ignorePath .gitignore --enableStatic",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@emotion/react": "^11.7.1",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.23.0",
    "github-markdown-css": "^4.0.0",
    "graphql": "^16.3.0",
    "highlight.js": "^11.2.0",
    "next": "^12.0.9",
    "pathpida": "^0.17.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "sharp": "^0.29.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.4",
    "@types/node": "^17.0.13",
    "@types/react": "17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "typescript": "4.4.3"
  }
}
