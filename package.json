{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:path": "yarn path --watch",
    "dev:generate": "npm run generate",
    "dev:next": "next dev",
    "prd": "run-s prd:*",
    "prd:path": "yarn path",
    "prd:generate": "npm run generate",
    "prd:build": "next build",
    "prd:next": "next start",
    "fix": "run-s fix:*",
    "fix:path": "yarn path",
    "fix:generate": "npm run generate",
    "fix:type": "yarn typecheck",
    "fix:lint": "yarn lint --fix",
    "fix:format": "yarn format",
    "lint-staged": "lint-staged --concurrent false",
    "path": "pathpida -o ./src/libs --ignorePath .gitignore --enableStatic",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --cache",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "date-fns": "^2.23.0",
    "github-markdown-css": "^5.1.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.3.0",
    "next": "^13.0.6",
    "pathpida": "^0.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "sharp": "^0.29.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/node": "^17.0.13",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.4.13",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^3.7.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.4.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.5.5"
  }
}
