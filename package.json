{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:path": "yarn path --watch",
    "dev:generate": "yarn generate --watch",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "run-s fix:*",
    "fix:path": "yarn path",
    "fix:generate": "yarn generate",
    "fix:type": "yarn typecheck",
    "fix:lint": "yarn lint --fix",
    "fix:format": "yarn format",
    "lint-staged": "lint-staged --concurrent false",
    "path": "pathpida -o ./src/libs --ignorePath .gitignore --enableStatic",
    "generate": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --cache",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "next": "^13.0.6",
    "pathpida": "^0.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "sharp": "^0.29.1"
  },
  "devDependencies": {
    "//": "encoding: https://github.com/vercel/next.js/issues/7621#issuecomment-810828589",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@types/node": "^17.0.13",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "autoprefixer": "^10.4.13",
    "encoding": "^0.1.13",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
